{
    "name": "thewebmen/silverstripe-installer",
    "description": "Uses the Webmen Default Silverstripe 4 Installer as base.",
    "require": {
        "php": ">=7.4",
        "silverstripe/recipe-plugin": "^1@stable",
        "silverstripe/recipe-cms": "^4@stable",
        "silverstripe/login-forms": "^4@stable",
        "thewebmen/silverstripe-elemental-grid": "^1@stable",
        "thewebmen/silverstripe-menustructure": "^1@stable",
        "thewebmen/silverstripe-elemental-media": "^1@stable",
        "thewebmen/silverstripe-media-field": "^1@stable",
        "jonom/focuspoint": "^3@stable",
        "cyber-duck/silverstripe-seo": "^4@stable",
        "silverstripe/display-logic": "^2@stable",
        "gorriecoe/silverstripe-linkfield": "^1@stable",
        "silverstripe/redirectedurls": "^2@stable"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2@stable",
        "symbiote/silverstripe-phpstan": "^4@stable",
        "phpstan/phpstan": "^0@stable",
        "phpunit/phpunit": "^5.7"
    },
    "extra": {
        "resources-dir": "_resources",
        "expose": [
            "themes/default/bundles",
            "themes/default/images",
            "app/css"
        ],
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
